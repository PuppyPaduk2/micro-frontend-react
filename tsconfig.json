{
  "compilerOptions": {
    "noEmit": true,
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "settings": ["./settings"],
      "api": ["./api"],
      "libs": ["./libs"],
      "services": ["./services"]
    }
  },
  "exclude": ["node_modules"],
}
